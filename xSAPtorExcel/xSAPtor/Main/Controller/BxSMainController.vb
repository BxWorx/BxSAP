			Friend	Function	SetActiveDestination(ByRef logonConfig As iLogonSystemDTO)	As Boolean _
													Implements iBxSMainController.SetActiveDestination

				Dim lb_Ret	As Boolean = True

				Try

						Me.co_ActiveDestination	= Me.co_CntlrNCO.Value.GetDestination(logonConfig.DestinationID)

						Me.co_ActiveDestination.Configure(	Client:=		logonConfig.Client,
																								User:=			logonConfig.UserName,
																								Password:=	logonConfig.Password,
																								Langauge:=	logonConfig.Language)

					Catch ex As Exception
						lb_Ret	= False

				End Try

				Return lb_Ret

			End Function

			'¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨
			Public	Async	Function	IsConnectedAsync() As Task(Of Boolean) _
																	Implements iBxSMainController.IsConnectedAsync

				Dim lb_Ret	As Boolean = False

				If Me.ActiveDestination IsNot Nothing
					
					lb_Ret	= Await Task.Run(	Function()	Me.ActiveDestination.Ping() )

				End If

				Return lb_Ret

			End Function
			'¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨¨
			Friend Async	Function	GetDestMonitorDataAsync()	As Task(Of List(Of iBxSDestMonitorDTO)) _
																Implements iBxSMainController.GetDestMonitorDataAsync

				If Me.ActiveDestination Is Nothing
						Return New List(Of iBxSDestMonitorDTO)
					Else	
						Return Await Task.Run(Function() Me.co_ActiveDestination.GetMonitorData() )
				End If

			End Function

